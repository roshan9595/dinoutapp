<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rosh.dineout" >

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application



        android:allowBackup="true"
        android:icon="@mipmap/dineout"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Signup"
            android:label="@string/title_activity_signup"

            android:theme="@style/Theme.AppCompat.Light.DarkActionBar"
          >
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"

            android:theme="@style/Theme.AppCompat.Light.DarkActionBar"
           >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".contentlist"
            android:label="@string/title_activity_contentlist" >
        </activity>
        <activity
            android:name=".card_layout"
            android:label="@string/title_activity_card_layout" >
        </activity>
        <activity
            android:name=".sorting"
            android:label="@string/title_activity_sorting" >
        </activity>
    </application>

</manifest>
